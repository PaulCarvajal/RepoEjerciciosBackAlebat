{
  "kind": "collectionType",
  "collectionName": "classes",
  "info": {
    "singularName": "class",
    "pluralName": "classes",
    "displayName": "Class"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "Title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Description": {
      "type": "blocks",
      "required": true
    },
    "Start_date": {
      "type": "datetime",
      "required": true
    },
    "events": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::event.event",
      "mappedBy": "classes"
    },
    "teachers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::teacher.teacher",
      "mappedBy": "classes"
    }
  }
}
